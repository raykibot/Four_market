<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luo.infrastructure.dao.IRaffleActivitySkuDAO">

    <select id="queryRaffleActivitySkuBySku" resultType="com.luo.infrastructure.pojo.RaffleActivitySku">
        select id,sku,activity_id,activity_count_id,stock_count,stock_count_surplus,create_time,update_time from
        raffle_activity_sku where sku = #{sku}
    </select>
</mapper>